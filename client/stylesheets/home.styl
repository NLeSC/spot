  text-align: justify;
}

.afCheckIcon {
  font-size: 40px;
}


.bgspot {
    background: url("../../../dist/../images/background.jpg")no-repeat center center;
    //background-color: hsl(200, 28%, 47%);
    //background-color: black;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
}

.bgspotTrans:after {
  content: "";
  background: url("../../../dist/../images/background.jpg")no-repeat center center;
  background-size:100%;
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  background-color:rgba(255,255,255,0.4);
  opacity: 0.8;
  background-size: 100% auto;
}

.footerimg{
  position: relative;
  margin: 0px 5px 0px 5px; // top, right, bottom, left
  float: left;
  width: 60px;
  height: 60px;
  background-position: 50% 50%;
  background-repeat:   no-repeat;
  background-size:     cover;
}


