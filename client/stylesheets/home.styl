.mdl-icon-toggle__label.material-icons {
  color: rgb(255, 255, 255);
}

.mdl-icon-toggle.is-checked .mdl-icon-toggle__label {
  color: rgb(32, 168, 216);
}
.mdl-checkbox.mdl-checked ._mdl-icon {
  background-color: white;
}


.afIcon {
  font-size: 40px;
